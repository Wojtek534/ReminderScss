{
  "name": "outsystemscss",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "bootstrap": "4.0.0-alpha.6",
    "chalk": "2.1.0",
    "font-awesome": "4.7.0",
    "jquery": "3.2.1",
    "npm-run-all": "4.1.1",
    "prop-types": "15.6.0",
    "react": "16.0.0",
    "react-dom": "16.0.0",
    "react-router-dom": "4.2.2",
    "react-scripts": "1.0.14",
    "react-transition-group": "2.2.1",
    "reactstrap": "4.8.0",
    "readmore-js": "2.1.0"
  },
  "scripts": {
